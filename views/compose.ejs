
<%- include partials/header %>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="">Title</label>
    <input type="text" class="form-control" name="postTitle" autocomplete="off">
    <label for="">Post</label>
    <textarea name="postBody" id="" cols="40" rows="10" class="form-control"></textarea>
